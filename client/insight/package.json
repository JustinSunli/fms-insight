{
  "name": "@seedtactics/insight-client",
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "private": true,
  "main": "./src/index.ts",
  "targets": {
    "main": false
  },
  "sideEffects": [
    "./src/index.ts",
    "./src/demo.ts"
  ],
  "browserslist": [
    "last 2 years",
    "> 1%",
    "not dead",
    "edge 18"
  ],
  "scripts": {
    "start": "parcel serve --dist-dir .parcel-dev/app src/index.html",
    "start-demo": "parcel serve --dist-dir .parcel-dev/demo src/demo.html",
    "build": "tsc --noEmit --composite false && parcel build --no-scope-hoist src/index.html && cp service-worker.js dist",
    "build-demo": "tsc --noEmit --composite false && parcel build --no-scope-hoist --dist-dir demo --public-url /demo/ src/demo.html",
    "test": "jest --ci --runInBand",
    "test:watch": "jest --watch",
    "nswag": "./node_modules/nswag/bin/nswag.js swagger2tsclient /runtime:NetCore20 /input:../../server/fms-insight-api.json /output:src/data/api.ts"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.3.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "copy-to-clipboard": "^3.0.0",
    "date-fns": "^2.21.3",
    "highlight.js": "^10.7.2",
    "immer": "^9.0.2",
    "jdenticon": "^3.1.0",
    "jsbarcode": "^3.11.4",
    "localforage": "^1.9.0",
    "moment": "^2.29.1",
    "oidc-client": "^1.11.5",
    "prelude-ts": "^1.0.2",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dates": "^21.0.0",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-qr-reader": "^2.1.0",
    "react-redux": "^7.2.4",
    "react-sortable-hoc": "^2.0.0",
    "react-timeago": "^6.1.1",
    "react-to-print": "^2.12.4",
    "react-vis": "^1.8.2",
    "recoil": "^0.3.1",
    "reconnecting-websocket": "^4.4.0",
    "redux": "^4.1.0",
    "reselect": "^4.0.0",
    "wouter": "^2.7.4"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.0.0-beta.2",
    "@parcel/transformer-webmanifest": "2.0.0-beta.2",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/faker": "^5.5.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.7",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dates": "^21.8.2",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "nswag": "^13.11.1",
    "parcel": "2.0.0-beta.2",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "resolutions": {
    "is-svg": "^4.2.2"
  },
  "jest": {
    "testURL": "http://localhost",
    "preset": "ts-jest",
    "modulePathIgnorePatterns": [
      "<rootDir>/.parcel-dev/",
      "<rootDir>/.parcel-cache/"
    ],
    "snapshotSerializers": [
      "<rootDir>/prelude-ts-snapshots.js"
    ]
  }
}
