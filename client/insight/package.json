{
  "name": "insight",
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "private": true,
  "sideEffects": [
    "./src/index.ts",
    "./src/demo.ts"
  ],
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "parcel src/index.html",
    "start-demo": "parcel src/demo.html",
    "build": "tsc --noEmit --composite false && parcel build src/index.html && cp service-worker.js dist",
    "build-demo": "tsc --noEmit --composite false && parcel build --out-dir demo --public-url /demo/ src/demo.html",
    "test": "jest --ci --runInBand",
    "test:watch": "jest --watch",
    "nswag": "./node_modules/nswag/bin/nswag.js swagger2tsclient /runtime:NetCore20 /input:../../server/fms-insight-api.json /output:src/data/api.ts"
  },
  "//": {
    "node-forge-dep": "only specify node-forge 0.10 until parcel 2, to fix security vuln"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.2.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "copy-to-clipboard": "^3.0.0",
    "date-fns": "^2.17.0",
    "highlight.js": "^10.6.0",
    "immer": "^8.0.1",
    "jdenticon": "^3.1.0",
    "jsbarcode": "^3.11.3",
    "localforage": "^1.9.0",
    "moment": "^2.29.1",
    "oidc-client": "^1.11.5",
    "prelude-ts": "^1.0.2",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dates": "^21.0.0",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.1.0",
    "react-qr-reader": "^2.1.0",
    "react-redux": "^7.2.2",
    "react-sortable-hoc": "^1.4.0",
    "react-timeago": "^5.2.0",
    "react-to-print": "^2.12.3",
    "react-vis": "^1.8.2",
    "recoil": "^0.1.2",
    "reconnecting-websocket": "^4.4.0",
    "redux": "^4.0.0",
    "reselect": "^4.0.0",
    "wouter": "^2.7.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dates": "^21.8.1",
    "@types/react-dom": "^17.0.1",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.22.0",
    "faker": "^5.4.0",
    "jest": "^26.6.3",
    "node-forge": "^0.10.0",
    "nswag": "^12.3.1",
    "parcel-bundler": "^1.9.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  },
  "jest": {
    "testURL": "http://localhost",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/.build/",
      "<rootDir>/.cache/"
    ],
    "moduleNameMapper": {
      ".*\\.css$": "<rootDir>/file-mock.js",
      ".*webrtc-adapter.*": "<rootDir>/file-mock.js"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      ".*\\.svg$": "<rootDir>/file-mock.js",
      ".*events-json.txt$": "<rootDir>/file-mock.js"
    },
    "preset": "ts-jest",
    "testMatch": null,
    "snapshotSerializers": [
      "<rootDir>/prelude-ts-snapshots.js"
    ]
  }
}
